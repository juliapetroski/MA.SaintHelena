@article{broadhurst16,
	abstract = {{In response to concerns over wastage and negative impacts on stocks, the damage (antennae loss) and collateral mortality of artisanally trawled-and-discarded juvenile seabob shrimp (Xiphopenaeus kroyeri) and the key factors explaining variability were assessed in the South Brazil Bight. During 20 deployments across 7 d of fishing, 1562 X. kroyeri (14.5 $\pm$ 3.1 mm carapace length, CL) were discarded into replicate on-board tanks at both the start and end of sorting, with some individuals monitored immediately for antennae loss and mortalities, and the rest assessed after 3 h. Virtually, all X. kroyeri discarded towards the end of sorting (mean $\pm$ SD air exposure of 15.9 $\pm$ 3.9 min) died. By comparison, those discarded at the start (2.7 $\pm$ 2.8 min) had variable temporal mortalities (total of 52.3\\% immediately vs. 66.2\\% after 3 h) which, like antennae loss (total mean $\pm$ SE of 50.4 $\pm$ 34.2\\%) were positively associated with haul duration/catch weight and deck exposure, and more frequent among individuals with soft than hard exoskeletons. Antennae loss was also negatively correlated with CL. The results support (i) improving trawl selectivity to reduce the catches of small, unwanted X. kroyeri and other bycatch (and therefore total weights of catches) and/or (ii) sorting catches in water (to minimize air exposure). Such modifications might be promoted through awareness of the potential harvest benefits to fishers associated with reducing unaccounted fishing mortality of the targeted species.}},
	author = {Broadhurst, Matt K. and Millar, Russell B. and Spach, Henry L. and Colombo, Nathalia},
	date-added = {2024-07-01 16:30:41 -0300},
	date-modified = {2024-07-01 16:31:27 -0300},
	doi = {10.1093/icesjms/fsw064},
	eprint = {https://academic.oup.com/icesjms/article-pdf/73/9/2364/31231153/fsw064.pdf},
	issn = {1054-3139},
	journal = {ICES Journal of Marine Science},
	keywords = {Discards},
	month = {05},
	number = {9},
	pages = {2364-2369},
	title = {Damage and mortality of juvenile seabob shrimp (Xiphopenaeus kroyeri) discarded in a tropical artisanal trawl fishery},
	url = {https://doi.org/10.1093/icesjms/fsw064},
	volume = {73},
